{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug DisruptorPatternExample",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/DisruptorPatternExample",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "Build Project with CMake",
            "miDebuggerPath": "/path/to/your/gdb",
            "miDebuggerArgs": [],
            "logging": {
                "trace": false,
                "traceResponse": false,
                "engineLogging": false,
                "programOutput": false,
                "exceptions": false
            },
            "linux": {
                "MIMode": "gdb"
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb"
            }
        }
    ]
}
